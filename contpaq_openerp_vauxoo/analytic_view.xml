<?xml version="1.0"?>
<openerp>
    <data>
        <record model="ir.ui.view" id="view_account_analytic_contract_code_form">
            <field name="name">analytic.analytic.account.contract.code.form</field>
            <field name="model">account.analytic.account</field>
            <field name="inherit_id" ref="analytic.view_account_analytic_account_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='code']" position="after">
                    <field name="vx_contract_code" readonly="True"/> 
                </xpath>
            </field>
        </record>
        <record model="ir.ui.view" id="view_account_analytic_account_list_inherit">
            <field name="name">account.analytic.account.list.inherit</field>
            <field name="model">account.analytic.account</field>
            <field name="inherit_id" ref="account.view_account_analytic_account_list"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='code']" position="after">
                    <field name="vx_contract_code"/> 
                </xpath>
            </field>
        </record>

    </data>
</openerp>
