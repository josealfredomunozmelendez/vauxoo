<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr="//link[last()]" position="after">
            <link href="/vauxoo/static/src/less/apps.less" rel="stylesheet" type="text/less"/>
            <link href="/vauxoo/static/src/css/apps.css" rel="stylesheet" type="text/css"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script src="/vauxoo/static/src/js/apps.js" type="text/javascript"></script>
        </xpath>
    </template>
    <template id="assets_backend" name="account assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" href="/vauxoo/static/src/less/dashboard.less"/>
            <script type="text/javascript" src="/vauxoo/static/src/js/widget.js"></script>
        </xpath>
    </template>
</odoo>
