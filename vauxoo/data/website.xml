<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="menu_about" model="website.menu">
        <field name="name">About</field>
        <field name="url">/page/website.aboutus</field>
        <field name="parent_id" ref="website.main_menu"/>
        <field name="sequence" type="int">200</field>
    </record>
    <record id="website.default_website" model="website">
        <field name="name">www.vauxoo.com</field>
        <field name="social_twitter">https://twitter.com/vauxoo</field>
        <field name="social_facebook">https://www.facebook.com/vauxoo</field>
        <field name="social_youtube">https://www.youtube.com/vauxoomedia</field>
        <field name="social_googleplus">https://plus.google.com/+vauxoo</field>
        <field name="social_linkedin">http://www.linkedin.com/company/vauxoo</field>
        <field name="social_github">http://vauxoo.github.io</field>
        <field name="favicon" type="base64" file="vauxoo/static/src/img/iconvauxoo.ico"/>
    </record>
    <record id="base.main_company" model="res.company">
        <field name="rml_header1">Opensource specialists.</field>
        <field name="logo" type="base64" file="vauxoo/static/src/img/banner.png"/>
    </record>
</odoo>
