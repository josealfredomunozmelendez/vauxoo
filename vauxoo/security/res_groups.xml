<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.module.category" id="module_category">
        <field name="name">Vauxoo</field>
        <field name="sequence">0</field>
        <field name="visible" eval="0" />
    </record>
    <record id="developer" model="res.groups">
        <field name="name">Developer</field>
        <field name="category_id" ref="module_category"/>
        <field name="implied_ids"
               eval="[(4, ref('project.group_project_user')),
(4, ref('base.group_user'))]"/>
    </record>
    <record id="manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="module_category"/>
        <field name="implied_ids"
               eval="[(4, ref('vauxoo.developer'))]"/>
    </record>
    <record id="directive" model="res.groups">
        <field name="name">Directive</field>
        <field name="category_id" ref="module_category"/>
        <field name="implied_ids"
               eval="[(4, ref('vauxoo.manager'))]"/>
    </record>
</odoo>
