<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <record id="ir_mail_server_localhost0" model="ir.mail_server">
            <field name="name">Vauxoo SMTP Server</field>
            <field name="smtp_host">smtp.mailgun.org</field>
            <field name="smtp_user">postmaster@runbot.vauxoo.com</field>
            <field name="smtp_pass">dummydummy</field>
            <field name="smtp_encryption">none</field>
            <field eval="465" name="smtp_port"/>
            <field eval="10" name="sequence"/>
        </record>

        <record id="runbot_config_settings0" model="runbot.config.settings">
            <field name="default_workers">6</field>
            <field name="default_running_max">75</field>
            <field name="default_timeout">1800</field>
            <field name="default_starting_port">2000</field>
            <field name="default_domain">runbot.vauxoo.com</field>
        </record>

        <record id="base.main_partner" model="res.partner" context="{'default_is_company': True}">
            <field name="name">Vauxoo S.A. de C.V.</field>
            <field name="company_id" eval="None"/>
            <field name="image" type="base64" file="runbot_vauxoo_com/static/img/vauxoo_logo.png"/>
            <field name="customer" eval="False"/>
            <field name="is_company" eval="True"/>
            <field name="street">Campobasso Norte 3206</field>
            <field name="city">Ciudad Obregon</field>
            <field name="country_id" ref="base.mx"/>
            <field name="zip">85134</field>
            <field name="phone">+52 (477) 773.33.46</field>
            <field name="email">info@vauxoo.com</field>
            <field name="website">www.vauxoo.com</field>
        </record>

        <record id="base.main_company" model="res.company">
            <field name="name">Vauxoo S.A. de C.V.</field>
            <field name="partner_id" ref="base.main_partner"/>
            <field name="logo" type="base64" file="runbot_vauxoo_com/static/img/vauxoo_logo.png"/>
            <field name="city">Ciudad Obregon</field>
            <field name="country_id" ref="base.mx"/>
            <field name="currency_id" ref="base.MXN"/>
        </record>
    </data>
</openerp>
