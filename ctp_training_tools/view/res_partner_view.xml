<?xml version="1.0"?>
<openerp>
    <data>
        <record model="ir.ui.view" id="res_partner_certificate_view">
            <field name="name">res.partner.form.certificate</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook/page[@string='Contacts']" position="after">
                    <page string="CTP">
                        <group>
                            <field name="certificate_code"/>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</openerp>
